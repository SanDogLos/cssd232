     function sca() {
        var a = [463, 464, 465, 466, 467, 468];
        var b = ['Звезда х5', 'Жена арестанта х5', 'Мешок х5', 'Решетка х5', 'Колокол воли х3', 'Отец народов х5'];
        var c = '';
        for (var i = 0; i < a.length; i++) c += (uach[a[i]] > 0 ? b[i] + '<br>' : '');
        return c;
    }

