function chLvl(a){var b=90,c=10;return a+=b,a<b?0:(-1*(2*b-c)+Math.sqrt((2*b-c)*(2*b-c)-4*c*(-2*a)))/(2*c)}
function chExp(a){var b=90,c=10;return(2*b+(a-1)*c)*a/2-b}
