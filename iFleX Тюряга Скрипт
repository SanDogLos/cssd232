// no comments ...
function tsFormat(a){var b=JSON.parse('{"Jan":"01", "Feb":"02", "Mar":"03", "Apr":"04", "May":"05", "Jun":"06", "Jul":"07", "Aug":"08", "Sep":"09", "Oct":"10", "Nov":"11", "Dec":"12"}'),c=new Date;c.setTime(a.toString().length>10?a:1e3*a);var d=c.toString().split(" ");return d[2]+"."+b[d[1]]+"."+d[3]+" в "+d[4]}

// timestamp
function tsCheck(a){return a-GM_getValue("update_ts",1)>6e5&&(GM_setValue("update_ts",a),!0)}
function initXML(a){var b=new DOMParser;return b.parseFromString(a,"text/xml")}

function printCoolDown(a,b,c,d){if(b<0&&10==c||b<0&&20==c||b<0&&900==d)return'<span style="color:#FF7777">мало крутизны шмота!</span>';if(a){var f,e=new Date,g=0,h=Math.floor(e.getTime()/1e3)-a;if(g=a+259200,f=g-e.getTime()/1e3,a=f>0&&c>0?f-b/c*d:f,h<21600&&c>0&&a<21600&&(a=21600-h),a>0){var j=0,k=0;return j=Math.floor(a/3600),a%=3600,k=Math.floor(a/60),j+" "+declOfNum(j,["час","часа","часов"])+" "+k+declOfNum(k,[" минуту"," минуты"," минут"])}return'<span style="color:#20A020;">доступно!</span>'}return"не известно"}
function declOfNum(a,b,c){return cases=[2,0,1,1,1,2],b[a%100>4&&a%100<20?2:cases[a%10<5?a%10:5]]}

// название камеры
function getCellName(number) {
	var backs = {
		'0': 'Обычная',
		'1': 'Кирпича',
		'2': 'Махно',
		'3': 'Лютого',
		'4': 'Шайбы',
		'5': 'Палёного',
		'6': 'Борзова',
		'7': 'Хирурга',
		'8': 'Борзова <span style="color:#F3505C;font-size:10px;padding:0 2px 0 6px;">авторитетная</span>',
		'9': 'Раисы',
