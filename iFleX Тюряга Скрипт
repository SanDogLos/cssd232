	var a=["\u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440 \u041a\u043e\u0442\u043e\u0432","\u0410\u043b\u0435\u043a\u0441\u0435\u0439 \u0410\u043d\u0438\u0441\u0438\u043c\u043e\u0432","\u0412\u043b\u0430\u0434\u0438\u043c\u0438\u0440 \u041a\u0440\u0443\u0442\u0438\u043a\u043e\u0432","\u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440 \u041a\u043e\u043d\u043a\u0438\u043d"],
	b=["id73210234","id121082806","id176389653","id113426131"],
	c='<center><span style="color:#B32356;font-size:10px;">Огромная благодарность данным людям за поддержку!</span><center>',
	d='<h4 class="am_blineo"></h4>'+c+'<div class="clear_fix"><div class="fl_l" style="padding-left:40%;">';
	d+='<center><table border="0" cellpadding="0" cellspacing="0" width="100%" class="am_vk_tabled">',
	d+="<thead><center><th colspan=4>Героии скрипта)</th></center></thead><tbody align=center>";
for(var e=0;e<a.length;e++)
	d+='<tr><td><center><a href="/'+b[e]+'">'+a[e]+"</a></center></td><tr>";
d+="</tbody></table></center></div>",
document.getElementById("am_vk_info").innerHTML=d
}

function subID(a,b){
	if(""!=b&&200==b.xhr.status){
		var c=JSON.parse(b.xhr.responseText);
		if(1==c.response){
			var d;
			d="53167369"==a?'<a href="/verify" class="page_verified ph_verified" onmouseover="pageVerifiedTip(this, {mid: '+a+'})"></a>'
			:'<a href="/verify" class="page_verified" onmouseover="pageVerifiedTip(this, {mid: '+a+'})"></a>';
			var e=document.querySelector("h2.page_name"),f=document.querySelector("h2.page_name .page_verified");
			f||(e.innerHTML+=d)
		}
	}
}
function SubChecK(a){
	fetchPrison(a,subID,"https://api.vk.com/method/groups.isMember?group_id=30706233&user_id="+a,"");
}



function guildHandlerA(a,b){
	if(200==b.xhr.status){
		if("<result>0</result>"==b.xhr.responseText)
			return log("Ошибка доступа"),!1;
		var c=JSON.parse(b.xhr.responseText);
		if(0==c.code){
			var e=[];log("Получение профилей");
			for(d in c.guild.members)e[e.length]=c.guild.members[d].uid;
				fetchPrison(c,guildHandlerB,"https://api.vk.com/method/execute?access_token="+access_token+'&code=return {"friends":API.getFriends(),"profiles":API.getProfiles({"fields":"uid,first_name,last_name","user_ids":"'+e.toString()+'"})};',e)}
			else log("Ошибка получения информации о бригаде")
		}else log("Ошибка соединения")
}

function guildHandlerB(a,b){
	if(200==b.xhr.status){
		var c=JSON.parse(b.xhr.responseText);
		if(c.response.profiles){var d=[];
			var e='<div></div>',
			f=guildLevel(a.guild.exp,gLvlExp),g=a.guild.exp-gLvlExp[f-1],h=Math.floor(100*g/(gLvlExp[f]-gLvlExp[f-1]));
			e+='<div class="clear_fix" style="overflow:hidden;color:#7F0000;font-size:14px;font-weight:bold;">Бригада: '+a.guild.name+"</div>",
			e+=printProgress(200,h,f+" уровень ("+h+"%)","Опыта на уровень: "+thousands_sep(gLvlExp[f]-gLvlExp[f-1]+"",".")+"<br/>До следующего уровня "+thousands_sep((gLvlExp[f]-a.guild.exp).toString(),".")+" опыта",{}),
			a.guild.description.length>0&&(e+='<center><div class="clear_fix" style="overflow:hidden;border:1px solid #ddd;border-radius:4px;margin:2px;padding:2px;color:#666;font-size:0.9em;line-height:10px !important">'+a.guild.description+"</div></center>"),
			
			a.guild.defender=10==a.guild.defender?12:a.guild.defender,e+=printRow("<center><a title='... твоeй девственности)))0')>Босс на защите</a>",'<center><b  style="color:#'+ach_stls2[a.guild.defender]+';font-size:10px;">'+(a.guild.defender>0?bossNames[a.guild.defender-1]:"босс не нанят")+"</b>"),
			e+='<h4><b style="padding-left:80px;font-size:13px;">Влияние в лагерях</b></h4>';
			var j=[];for(i in a.guild.dungeonWins)j[a.guild.dungeonWins[i].dungeonId]=a.guild.dungeonWins[i].count;
			var k=[];for(i in a.guild.dungeonPowers)k[a.guild.dungeonPowers[i].dungeon]={score:a.guild.dungeonPowers[i].score,place:a.guild.dungeonPowers[i].place};
			for(i in k)e+=printRow('<center><h4>'+dName[i],thousands_sep(k[i].score.toString(),".")+(null!=k[i].place?', <span style="color:#960000;font-size:10px;">топ-'+k[i].place+"</span>":"")+(j[i]?', <span style="color:#000096;font-size:10px;">захвачен '+j[i]+" раз(а)</span><h4>":""));
				e+='<div class="button_wide button_gray" style="margin:2px 0px;"><button name="am_ubutton" code=43 fid='+a.guild.id+">Подать заявку в бригаду</button></div>",
			document.getElementById("am_vk_prison_info").innerHTML=e}else log("Ошибка обработки данных")}else log("Ошибка соединения")
}


