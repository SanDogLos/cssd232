function injectScript(a,b,c,d){
	var e=document.getElementsByTagName(b)[0],
		f=document.createElement("script");
		f.setAttribute("type","text/javascript"),
		f.setAttribute("src",a),
		c&&(f.setAttribute("id",c),d&&document.querySelector("#"+c))||e&&e.appendChild(f)
}

