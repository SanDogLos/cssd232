var Url={
	protect:function(a){return a.replace(/</g,"&lt;").replace(/>/g,"&gt;")},
	decode:function(a){return this._utf8_decode(unescape(a))},
	_utf8_decode:function(a){
		for(var b="",c=0,d=c1=c2=0;c<a.length;)
			d=a.charCodeAt(c),d<128?(b+=String.fromCharCode(d),c++)
									:d>191&&d<224?(c2=a.charCodeAt(c+1),b+=String.fromCharCode((31&d)<<6|63&c2),c+=2)
									:(c2=a.charCodeAt(c+1),c3=a.charCodeAt(c+2),b+=String.fromCharCode((15&d)<<12|(63&c2)<<6|63&c3),c+=3);
	return b}
};


