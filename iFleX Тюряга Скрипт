function cityAct(a,b,c){
	for(var d=initXML(b.xhr.responseText),
		e=["id","name","mastery","darkmastery","leader","lovepeople","darkleader","darklovepeople"],
		f={},g=[],h=d.evaluate("//city",d,null,XPathResult.ANY_TYPE,null),
		j=h.iterateNext();j;){
			var k=j.getElementsByTagName("display_order")[0].textContent;f[k]={};
			for(i in e){
				var l=j.getElementsByTagName(e[i]);
				l[0]&&0!=l[0].textContent&&(f[k][e[i]]=l[0].textContent)
			}
		j=h.iterateNext()}document.getElementById("am_vk_prison_info").innerHTML='<div id="am_vk_cities" class="am_vk_scb" style="height: 455px;overflow-y: scroll;"></div>';
		for(i in f)
			document.getElementById("am_vk_cities").innerHTML+=printCity(f[i],userCities[f[i].id]),f[i].leader&&g.push(f[i].leader),f[i].darkleader&&g.push(f[i].darkleader);
		fetchPrison(null,function(a,b){
			if(200==b.xhr.status){
				var c=JSON.parse(b.xhr.responseText);
					if(c.response)
						for(var d=0;d<c.response.length;d++)
						for(var e=document.querySelectorAll('div>a[data-luid="'+c.response[d].uid+'"]'),f=0;f<e.length;f++)
						e[f].href="/id"+c.response[d].uid,e[f].title=c.response[d].first_name+" "+c.response[d].last_name,e[f].firstChild.src=c.response[d].photo_50}},
						"http://api.vk.com/method/users.get?fields=photo_50&user_ids="+g.join(","),null)
}

function getDataAct(a,b,c){
	b&&(getData=JSON.parse(b.xhr.responseText));
	var d={sets:{}};
	for(i in getData.sets)
		getData.sets[i].name.length&&(d.sets[getData.sets[i].id]={name:getData.sets[i].name,total:0,have:0});
	for(i in getData.items)
		d.sets[getData.items[i].set_id]&&(d.sets[getData.items[i].set_id].total+=1,
			userItems.indexOf(getData.items[i].id)!=-1&&(d.sets[getData.items[i].set_id].have+=1));
	document.getElementById("am_vk_prison_info").innerHTML="";
	for(i in d.sets)document.getElementById("am_vk_sets").innerHTML+=printSRow(d.sets[i].name,d.sets[i].have,d.sets[i].total)
}


function cities(a){
	var b=a.iterateNext(),
	c={liking:2,missionLevel:3,darkLiking:4,darkMissionLevel:5};
	for(rs=[];b;){
		var d=b.getElementsByTagName("cityId")[0].textContent;rs[d]={};
		for(v in c){
			var e=b.getElementsByTagName(v);
			rs[d][v]=e[0]?e[0].textContent:0
		}
		b=a.iterateNext()
	}
	return rs
}

function printCity(a,b){
	var c='<div class="clear_fix fl_l" style="width: 50%;"><div class="fl_l" style="width: 60px;"><a data-luid=leader><img src="/images/camera_c.gif" title=""></a><div>lovepeople</div></div><div class="clear_fix "><div class="label fl_l" style="width: 60px;">Ходки:</div><div class="labeled fl_l" style="width: 50px;">missionLevel</div><div class="label fl_l" style="width: 60px;">Уважение:</div><div class="labeled fl_l" style="width: 50px;">liking</div></div></div>',
	d='<div class="clear_fix" style="margin: 2px;padding: 2px;border: 1px solid lightgray;background-color: floralwhite;"><div style="text-align: center;font-weight: bold;">name</div>';
	return d=d.replace("name",a.name),
	d+=c.replace("name",a.name)
		.replace("lovepeople",a.lovepeople||0)
		.replace("leader",a.leader||0)
		.replace("missionLevel",b&&b.liking?b.missionLevel:0)
		.replace("liking",b&&b.liking||0),
	a.darklovepeople&&(d+=c.replace("lovepeople",a.darklovepeople)
		.replace("leader",a.darkleader||0)
		.replace("missionLevel",b&&b.darkLiking?b.darkMissionLevel:0)
		.replace("liking",b&&b.darkLiking||0)),d+"</div>"
}

