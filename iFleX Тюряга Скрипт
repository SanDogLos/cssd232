function nstHandler(a,b){
	if(a=document.getElementById(a),200==b.xhr.status){
		var c=JSON.parse(b.xhr.responseText);
		a.className="msg",a.style["text-align"]="center";
		var d="false"!=nst?"":"Поддержка НСТ отключена, возможно данные устарели";
		if(c.response){
			var e={},f=0;
			for(i in nstBase)c.response.indexOf(i-0)>=0&&(e[i]=nstBase[i],f++);
				if(f){
					var g="<center>"+d+'</center><div class="clear_fix am_nstlinks">';
					g+='<center><table border="0" cellpadding="0" cellspacing="0" width="100%" class="am_vk_tabled">',
					g+="<thead><th colspan=2>Кидалы в друзьях</th></thead><tbody align=center>";
					for(i in e)g+='<tr><td><center><a href="/id'+i+'"> vk.com/id'+i+" (Дело №"+e[i][0]+")</a></center></td></tr>";
						g+="</tbody></table></center>",
						document.getElementById("am_vk_prison_info").innerHTML=g+"</div>"
				}
		else a.innerHTML='<b style="color:green">Кидал в друзьях не обнаружено</b><br class="clear">'+d}
		else a.innerHTML="Error "+c.error.error_code+": "+c.error.error_msg}else console.log("nstHandler connect error")
}


function guildAct(a,b){
	a.id||(a=document.getElementById(a));
	var c=JSON.parse(b.xhr.responseText),d=!1;
	if(0==c.code)
		if(b.fid){
			a.innerHTML+="<br/>OK!";
			for(var e=0;e<c.requests.length;e++)
				if(c.requests[e].uid==b.fid){
					switch(a.innerHTML+="Обработка...",d=!0,b.href){
						case"40":fetchPrison(a.id,guildAct,"http://"+srv_ip[srv_sel]+"/prison/universal.php?method=guild.declineJoinRequest&user="+user+"&key="+auth_key+"&request="+JSON.stringify(c.requests[e]),b.href);break;
						case"41":fetchPrison(a.id,guildAct,"http://"+srv_ip[srv_sel]+"/prison/universal.php?method=guild.acceptJoinRequest&user="+user+"&key="+auth_key+"&request="+JSON.stringify(c.requests[e]),b.href)}break}
		(c.requests.length<1||!d)&&(a.innerHTML='<b style="color:#11aa11">'+(d?"0 заявок":"Нет заявки")+"</b>")}
		else a.innerHTML='<b style="color:#11aa11">Выполнено!</b>';
		else a.innerHTML='<b style="color:#aa1111">Ошибка</b>'
}

function usefullHandler(a,b){
	if(a=document.getElementById(a),200==b.xhr.status){
		if("<result>0</result>"==b.xhr.responseText)
			return a.innerHTML='<b style="color:#aa1111">Ошибка соединения</b>',!1;
		if(a.className="msg",a.style["text-align"]="center",9==b.href)
			return a.innerHTML='<b style="color:#11aa11">Успешно отправлено '+(b.xhr.responseText.match(/success save/g)||[]).length+"</b>",!1;
		if(b.href>10)
			return b.href<40?(a.innerHTML=b.xhr.responseText.indexOf(">success vote<")>=0?'<b style="color:#11aa11">Ништяк!</b>':'<b style="color:#aa1111">Не получилось!</b>',!1):guildAct(a,b);
		if(4!=b.href)b.xhr.responseText.indexOf("<data></")>=0||b.xhr.responseText.indexOf('code":0')>=0||b.xhr.responseText.indexOf("<code>0<")>=0?a.innerHTML='<b style="color:#11aa11">Выполнено!</b>':a.innerHTML='<b style="color:#aa1111">Ошибка</b>';
		else{var c=b.xhr.responseText.match(/[a-z]+>[0-9]+/g),d=c[0].split(">")[1];d<28800?a.innerHTML='<span style="color:#000096">До сбора: '+sec2time(d)+"</span>":a.innerHTML='<b style="color:#aa1111">Можно собирать!</span>'}
}}

function printGStats(a){
	a.innerHTML="";
	for(var b=1;b<5;b++)
		a.innerHTML+=printURow({id:b,cnt:gstats[b][0]+" / "+gstats[b][1]+" / "+gstats[b][2]});
		a.innerHTML+='<span style="color:gray">Первая '+(gstats.inout?"отправл":"получ")+'ена:</span><br/><span style="font-size:10px;">'+(gstats.first<2?"всё еще впереди":tsFormat(gstats.first))+"</span><br/>",
		a.innerHTML+='<span style="color:gray">Последняя '+(gstats.inout?"отправл":"получ")+'ена:</span><br/><span style="font-size:10px;">'+(gstats.last>9e9?"во сне":tsFormat(gstats.last))+"</span><br/>",
		a.innerHTML+='<span style="color:gray">Всего: </span><span style="font-size:10px;">'+gstats.total+"</span>"
}

function xGet(a){
	if(!a)return!1;
	for(var b={},c=a.iterateNext();c;)
		b[c.nodeName]?b[c.nodeName].push?b[c.nodeName].push(c.textContent):b[c.nodeName]=[b[c.nodeName],c.textContent]:b[c.nodeName]=c.textContent,c=a.iterateNext();
	return b
}

function xAtt(a){
	if(!a)return !1;
	for(var b={},c=a.iterateNext();c;){
		for(var d=0;d<c.attributes.length;d++){
			var e=c.attributes[d].nodeName,
			f=c.attributes[d].nodeValue;
			"type"==e&&(b[f]?b[f]+=c.textContent:b[f]=c.textContent),
			"id"==e&&(b[f]?b[f]+=c.textContent:b[f]=c.textContent)
		}
		c=a.iterateNext()
	}
	return b
}

function sec2time(a,b){var c=a/3600^0,d=(a-3600*c)/60^0,e=a-3600*c-60*d;return(c<10?"0"+c:c)+(b||" ч. ")+(d<10?"0"+d:d)+(b||" мин. ")+(e<10?"0"+e:e)+(b?"":" сек.")}
function randA(a,b){for(var c=0;!c;)c=parseInt(Math.random().toString().substr(2,8));return c}
function uidRand(){for(var a=[],b=0;b<10;b++)a[a.length]=randA();return a.toString()}

