//форматирование числа тысячами с точкой
function thousands_sep(a,b){
	if(a.length<=3)
		return a;
	for(var c=a.length,d="",e=0,f=c-1;f>=0;f--){
		var g=a.substr(f,1);
		e%3!=0||0==e||isNaN(parseFloat(g))||(d=b+d),d=g+d,e++
	}
	return d
}

function isNumber(a){return!isNaN(parseFloat(a))&&isFinite(a)}

